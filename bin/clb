#!/bin/bash

function start {
    cl work nlp::rl
}

function upload {
	cl upload program /Users/JohnMiller/Documents/projects/rl/rl/ -L
}

function ex {
    cl run "export PYTHONPATH=%:Theano%:$PYTHONPATH; \
    		export THEANO_FLAGS=blas.ldflags=-lopenblas; \
    		python %:rl%/$1 $2" \
    -n $3 \
    --request_priority 1023 \
    --request_queue $4 \
    --request_cpus $5 \
    --request_memory $6
}

function bk {
	cl rm ^1 ^2
}

"$@"